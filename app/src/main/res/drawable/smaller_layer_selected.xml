<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="10dp" />
            <padding android:bottom="11dp"/>
            <padding android:left="11dp" />
            <padding android:right="11dp"/>
            <padding android:top="11dp"/>
            <stroke
                android:color="@color/darkTeal"
                android:width="3dp"/>
        </shape>
    </item>

</layer-list>